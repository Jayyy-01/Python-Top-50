num = int(input('Enter a number to check if it is a prime number or not: '))
if num <= 1:
    print('Not a prime number')
elif num > 1:
    for i in range(2, (num//2) + 1):
        if (num % i) == 0:
            print(num, 'is not a prime number')
            break
    else:
        print(num, 'is a prime number')
else:
    print('Invalid input')
            
