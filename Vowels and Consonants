word = input('Enter a word to the number of vowels and consonants in it: ')
vowels = ['a','e','i','o','u','A','E','I','O','U']
vowel_count = 0
cons_count = 0
for i in range(len(word)):
    if word[i] in vowels:
        vowel_count+=1
    else: 
        cons_count+=1
print('No. of vowels: ',vowel_count)
print('No. of consonants: ',cons_count)
